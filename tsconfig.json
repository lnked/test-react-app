{
    "compilerOptions": {
        "outDir": "./dist",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react"
    },
    "files": [
        "./app/typings/**.ts"
    ],
    "include": [
        "app/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "tests",
        "webpack",
        "**/*.spec.ts"
    ]
}
